<div class="sign-up">
    <form class="sign-up__form" action={{onEnter}}>
        <div class="sign-up__form_title">{{title}}</div>
        <div class="sign-up__form_inputs">
            {{{email}}}
            {{{login}}}
            {{{firstName}}}
            {{{secondName}}}
            {{{phone}}}
            {{{password}}}
            {{{passwordRetry}}}
        </div>
        {{{signUp}}}
        <div class="sign-up__form_link">{{{link}}}</div>
    </form>
</div>
